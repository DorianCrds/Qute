#custom_mainwindow {
    background-color: #1e1f22;
    color: #f1f3f5;
}

#custom_container_widget {
    background-color: #2c2e33;
    border: 1px solid #414449;
    border-radius: 12px;
    padding: 12px;
}

QLabel {
    color: #f1f3f5;
}

#custom_pushbutton {
    background-color: #4b5b78;
    color: #f1f3f5;
    border: 1px solid #6c7a92;
    padding: 6px 12px;
    border-radius: 6px;
}

#custom_pushbutton:hover {
    background-color: #5a6c8d;
}

